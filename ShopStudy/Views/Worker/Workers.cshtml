@model IEnumerable<WorkerModel>

@{
    ViewData["Title"] = "Workers";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Workers</title>
</head>
<body>
    <div class="container">
        @foreach (var item in Model)
        {
            <ol>
                <li>@Html.ActionLink(item.FirstName, "WorkerDetails", new { id = item.Id }) @item.SurName</li>
                <li>Возраст: @item.Age</li>
            </ol>
        }

        @Html.ActionLink("Вернуться назад", "Index", "Home")
    </div>
</body>
</html>
